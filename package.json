{
  "name": "sunday",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start:be-local": "concurrently 'docker-compose --env-file ./backend/.env.local -f docker-compose.local.yaml up --build' 'cd backend && npm run start:dev'",
    "start:be-prod": "docker-compose --env-file ./backend/.env.docker -f docker-compose.yaml up --build",
    "start:db": "docker-compose --env-file ./backend/.env.local -f docker-compose.local.yaml up --build",
    "start:nest": "cd backend && npm run start:dev",
    "start:fe": "cd frontend && npx ng serve",
    "start:fe-ssr-dev": "cd frontend && npm run dev:ssr",
    "start:fe-prod": "cd frontend && npm run start:prod",
    "lint": "eslint **/*.ts",
    "format": "prettier --write **/*.{ts,html,scss}",
    "update:validations": "cd libs/validations && rm -rf dist && npx tsc",
    "deploy:prod": "npm run start:be-prod & npm run start:fe-prod & sudo systemctl start cloudflared"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/brYordanov/sunday.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/brYordanov/sunday/issues"
  },
  "homepage": "https://github.com/brYordanov/sunday#readme",
  "devDependencies": {
    "concurrently": "^9.1.2"
  },
  "dependencies": {
    "axios": "^1.8.4",
    "cheerio": "^1.0.0",
    "g": "^2.0.1",
    "ts": "^0.2.2"
  }
}
